query listInstalledPackages($machineId: String!) {
  listInstalledPackages(machineId: $machineId) {
    name
    version
    description
    installed
    publisher
    source
  }
}

query searchPackages($machineId: String!, $query: String!) {
  searchPackages(machineId: $machineId, query: $query) {
    name
    version
    description
    installed
    publisher
    source
  }
}